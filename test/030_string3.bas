10 X = ASC("123")
20 Y = ASC(RIGHT$("123", 2))
30 Z = ASC(RIGHT$("123", 1))
60 B$=CHR$(X)+CHR$(Y)+CHR$(Z)
80 A$=STR$(-1*123)
140 PRINT B$, A$, "X ="; X; "Y ="; Y; "Z =";Z
150 GOSUB 1000
160 GOSUB 1100
180 IF A$ <> B$ THEN 900
190 IF A$ = B$ THEN PRINT "SUCCESS"
200 GOTO 999

900 PRINT "FAILED"
999 END

1000 REM TRIMLEFT A$
1010 IF ASC(A$) = 45 THEN 1030
1020 IF ASC(A$) <> 32 THEN RETURN
1030 A$=RIGHT$(A$, LEN(A$)-1)
1040 GOTO 1000
1050 RETURN

1100 REM TRIMRIGHT A$
1110 IF ASC(RIGHT$(A$, 1)) <> 32 THEN RETURN
1120 A$=LEFT$(A$, LEN(A$)-1)
1040 GOTO 1100
1050 RETURN
