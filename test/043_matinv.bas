10 REM IN-PLACE TRANSPOSE 3 X 3 MATRIX
20 OPTION BASE 0
30 DIM A(3,3)
40 REM LOAD DATA FOR A
50 DATA 1,2,3,4,5,6,7,8,9
60 FOR I=0 TO 2
70 FOR J=0 TO 2
80 READ A(I,J)
90 NEXT J
100 NEXT I
110 PRINT "ORIGINAL 3 X 3 MATRIX"
120 GOSUB 240
130 REM DO THE IN-PLACE TRANSPOSE
140 FOR I=0 TO 2
150 FOR J=I+1 TO 2
160 LET T=A(I,J)
170 LET A(I,J)=A(J,I)
180 LET A(J,I)=T
190 NEXT J
200 NEXT I
210 PRINT "TRANPOSED 3 X 3 MATRIX"
220 GOSUB 240
230 GOTO 320
240 REM PRINT MATRIX
250 FOR I=0 TO 2
260 FOR J=0 TO 2
270 PRINT A(I,J),
280 NEXT J
290 PRINT
300 NEXT I
310 RETURN
320 END

