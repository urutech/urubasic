100 REM COMPUTE FACTORIALS OF NUMBERS 0 TO 9.
107 OPTION BASE 0
108 DIM S(9)
110 LET D = 0
120 FOR J = 0 TO 9
130    LET N = J
140    GOSUB 1000
150    PRINT J;"! = ";F
160 NEXT J
170 STOP
1000 REM FACTORIAL SUBROUTINE
1010 IF N > 1 THEN 1040
1020   LET F = 1
1030   RETURN
1040 LET D = D + 1
1050 LET S(D) = N
1060 LET N = N - 1
1070 GOSUB 1000
1080 LET F = F * S(D)
1090 LET D = D -1
1100 RETURN
1110 END

